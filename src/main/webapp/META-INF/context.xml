<?xml version='1.0' encoding='utf-8'?>
<Context>
    <Resource
            name="jdbc/gestionClients"
            auth="Container"
            type="javax.sql.DataSource"
            driverClassName="com.mysql.cj.jdbc.Driver"
            username="${mysql.user}"
            password="${mysql.password}"
            url="${mysql.url}gestionClients?serverTimezone=UTC"
    />
</Context>